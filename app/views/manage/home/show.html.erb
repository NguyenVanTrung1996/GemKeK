  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Data Tables
        <small>advanced tables</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Tables</a></li>
        <li class="active">Data tables</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-header">
              <h3 class="box-title"><strong>Manage User</strong></h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Email</th>
                  <th>full_name</th>
                  <th>register date</th>
                  <th>number of reviews</th>
                  <th>number of comment</th>
                  <th>Function</th>
                </tr>
                </thead>
                <tbody>
                <% @user = User.all %>
                <% @user.each do |user| %>
                <% if not user.is_admin %>
                <tr>
                  <td><%= link_to user_profile_home_path(id: user.id) do %> <%= user.email%> <% end %> </td>
                  <td><%= user.full_name %>
                  </td>
                  <td><%= user.created_at.to_date %></td>
                  <td><%= user.reviews.count() %></td>
                  <td><%= user.comments.count() %></td>
                  <%= render "manage/users/block", user: user %>
                  <!--<% if user.is_blocked == false %>-->
                  <!--<td><%= link_to manage_user_block_path(user.id), method: :get do %> block <% end %></td>-->
                  <!--<% else %>-->
                  <!--<td><%= link_to manage_user_unblock_path(user.id), method: :get do %> unblock <% end %></td>-->
                  <!--<% end %>-->
                  
                </tr>
                <% end %>
                <% end %>
               
                </tfoot>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
